    "name": "Tony Crowe",
    "url": "https://tcrowe.github.io/contact"
  },
  "homepage": "https://github.com/tcrowe/webextension-infogalactic#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tcrowe/webextension-infogalactic.git"
  },
  "bugs": {
    "url": "https://github.com/tcrowe/webextension-infogalactic/issues"
  },
  "keywords": [],
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-loader": "^8.0.6",
    "chalk": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylus": "^2.7.0",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "remark-cli": "^7.0.1",
    "remark-lint": "^6.0.5",
    "remark-toc": "^6.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^4.0.0-beta.1"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "setup": "mkdir -p dist",
    "dev-eslint": "nodemon -L -d 1 -w src --exec eslint --cache --fix src",
    "dev-gulp": "NODE_ENV=development gulp dev || true",
    "dev": "run-p setup 'dev-*'",
    "prd-eslint": "eslint --fix gulpfile.js webpack.config.js src",
    "prd-prettier": "prettier --write gulpfile.js webpack.config.js src/**/*.js",
    "prd-gulp": "NODE_ENV=production gulp prd || true",
    "prd-remark": "remark . -o",
    "prd": "run-s clean setup 'prd-*'"
  },
  "remarkConfig": {
    "settings": {
      "bullet": "+",
      "listItemIndent": 1,
      "fences": true,
      "emphasis": "*"
    },
    "plugins": [
      "lint",
      [
        "toc",
        {
          "tight": true
        }
      ]
    ],
    "output": true
  }
}
