{
  "name": "webextension-infogalactic",
  "version": "0.1.0",
  "description": "toggle redirect mode",
  "homepage_url": "https://github.com/tcrowe/webextension-infogalactic",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "webextension-infogalactic",
    "default_popup": "popup.html"
  },
  "permissions": ["<all_urls>", "storage", "webRequest", "webRequestBlocking"],
  "icons": {
    "32": "favicon.ico"
  }
}
